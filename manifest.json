{
  "name": "Skeleton extension",
  "version": "0.1",
  "description": "A skeleton extension to get started",
  "background": {
    "scripts": [ "background.js" ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "commands": {
    "example_keyboard_command": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Ctrl+Shift+S"
      },
      "description": "Do the thing"
    }
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "manifest_version": 2
}
